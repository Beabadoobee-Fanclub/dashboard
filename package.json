{
	"name": "webpage",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "yarn run build && wrangler dev",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "yarn run build && wrangler deploy -e production",
		"stage": "yarn run build && wrangler deploy -e staging",
		"cf-typegen": "wrangler types ./src/worker-configuration.d.ts"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.10.2",
		"@flowbite-svelte-plugins/chart": "^0.2.4",
		"@flowbite-svelte-plugins/texteditor": "^0.25.1",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-cloudflare": "^7.1.1",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@tiptap/core": "3.0.7",
		"discord-api-types": "^0.37.119",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.10.12",
		"flowbite-svelte-icons": "^2.2.1",
		"lowlight": "^3.3.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"wrangler": "^4.26.1"
	}
}